{
  "extends": "eventbrite",
  "env": {
    "browser": true,
    "node": true,
    "shared-node-browser": true,
    "amd": true
  },
  "globals": {
    "Promise": true,
    "Symbol": true
  },
  "rules": {
    "import/no-dynamic-require": 0,
    "no-shadow": 0,
    "import/no-commonjs": [
      0
    ],
    "import/unambiguous": [
      0
    ],
    "no-console": [
      0
    ],
    "func-style": [
      0
    ],
    "one-var": [
      0
    ],
    "no-var": [
      0
    ]
  }
}
